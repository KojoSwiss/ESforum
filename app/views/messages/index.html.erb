<% @messages.each do |message| %>
<div class="row justify-content-center">
  <div class="col-md-10 col-md-offset-1">
    <div class="message">
      <h2 class="message-title"><%= message.title %></h2>
      <%= link_to "Join Convo", message, class: "btn-custom" %>
    </div>
  </div>
</div>
<% end %>
